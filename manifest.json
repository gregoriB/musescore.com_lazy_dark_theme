{
  "manifest_version": 2,

  "name": "Musescore Lazy Dark Theme",
  "version": "0.1.0",
  "description": "A very lazy dark theme for musescore.com",

  "content_scripts": [
    {
      "matches": ["https://musescore.com/*"],
      "css": ["main.css"]
    }
  ],

  "page_action": {
    "default_icon": "icon.png"
  },
  
  "permissions": ["activeTab"]
}